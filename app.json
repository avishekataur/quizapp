{
  "expo": {
    "name": "QuizPro",
    "slug": "quiz-pro",
    "description":"This Is An Awesome Quiz App.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/quizpro_logo.png",
    "splash": {
      "image": "./assets/quizpro_banner.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
